<%@ page import="net.ech.anagram.*" %><%@ page import="net.ech.apps.anagram.*" %><%@ page import="java.util.*" %><% AnagramCollector coll = (AnagramCollector) request.getAttribute("anagram"); %><% if (coll.getSize() == 0) { %>NO RESULTS<% } else { for (Iterator<String[]> iter = coll.iterator(); iter.hasNext(); ) { String[] words = (String[]) iter.next(); for (int i = 0; i < words.length; ++i) { %><%= (i > 0 ? " " : "") + words[i] %><% } %><%= '\n' %><% }} %>
